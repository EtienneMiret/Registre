<persistence xmlns="http://java.sun.com/xml/ns/persistence"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
  version="2.0">
  <persistence-unit name="Registre">
    <provider>org.hibernate.ejb.HibernatePersistence</provider>

    <!-- Classes à persister. -->
    <class>fr.elimerl.registre.model.Acteur</class>
    <class>fr.elimerl.registre.model.BandeDessinée</class>
    <class>fr.elimerl.registre.model.Compositeur</class>
    <class>fr.elimerl.registre.model.Dessinateur</class>
    <class>fr.elimerl.registre.model.Emplacement</class>
    <class>fr.elimerl.registre.model.Fiche</class>
    <class>fr.elimerl.registre.model.Film</class>
    <class>fr.elimerl.registre.model.Mot</class>
    <class>fr.elimerl.registre.model.Nommé</class>
    <class>fr.elimerl.registre.model.Propriétaire</class>
    <class>fr.elimerl.registre.model.Réalisateur</class>
    <class>fr.elimerl.registre.model.Référence</class>
    <class>fr.elimerl.registre.model.Scénariste</class>
    <class>fr.elimerl.registre.model.Série</class>
    <class>fr.elimerl.registre.model.Session</class>
    <class>fr.elimerl.registre.model.Utilisateur</class>
    <exclude-unlisted-classes/>

  </persistence-unit>
</persistence>
